<h1>Picklepals 2024 Q2</h1>

<table class="table table-striped leaderboard">
  <thead>
    <tr>
      <th scope="col" class="col-xs-1">#</th>
      <th scope="col" class="col-xs-6 col-md-7">Name</th>
      <th scope="col" class="col-xs-3 col-md-2 leaderboard_record">Record</th>
      <th scope="col" class="col-xs-2 col-md-2 leaderboard_rating">Rating</th>
    </tr>
  </thead>
  <tbody>
    <% if @players.any? %>
      <%= render @players %>
    <% end %>
  </tbody>
</table>


